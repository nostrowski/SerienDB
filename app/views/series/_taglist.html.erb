<font color="<%=
if @a_series.users_owning_series_complete.include?(User.current) then
	"green"
else
	if @a_series.users_owning_series_partly.include?(User.current) then
		"orange"
	else
		"red"
	end
end %>
">X</font>
<% @a_series.tags.order("priority DESC").each do |tag| %>
	<font color="<%= tag.color %>">
	<%= tag.acronym %>
	</font>
<% end %>
